{
  "name": "spryker/merchant-relationship-gui",
  "type": "library",
  "description": "MerchantRelationshipGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/company": "^1.0.0",
    "spryker/company-business-unit": "^1.0.0 || ^2.0.0",
    "spryker/gui": "^3.13.0",
    "spryker/kernel": "^3.30.0",
    "spryker/merchant": "^1.1.0 || ^2.3.0 || ^3.0.0",
    "spryker/merchant-relationship": "^1.7.0",
    "spryker/merchant-relationship-gui-extension": "^1.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-text": "^1.1.1"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
